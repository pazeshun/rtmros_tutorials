<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find hironx_tutorial)/config/hironxjsk_gazebo_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="pr2_controller_manager"
        type="spawner" respawn="false"
        output="screen" args="larm_controller
                              rarm_controller
                              head_controller
                              torso_controller
                              lhand_controller
                              rhand_controller "/>
  <node name="go_initial" pkg="hironx_tutorial"  type="go_initial_gazebo.py" />
</launch>
